{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "command": "../.rbenv/shims/bundle",
  "args": ["exec", "jekyll"],
  "tasks": [
    {
      "args": [
        "clean"
      ],
      "label": "clean"
    },
    {
      "args": [
        "serve",
        "--incremental",
        "--trace"
      ],
      "label": "serve",
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}